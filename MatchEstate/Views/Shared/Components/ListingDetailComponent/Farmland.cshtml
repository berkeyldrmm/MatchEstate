@using Shared.Dtos.PropertyListing.Detail
@model FarmlandListingDetailDto

<li class="row">
    <strong class="col-5">Square Metersize </strong>|
    <span class="col-6">@Model.SquareMetersize</span>
</li>
<li class="row">
    <strong class="col-5">Block Number </strong>|
    <span class="col-6">@Model.BlockNumber</span>
</li>
<li class="row">
    <strong class="col-5">Parsel Number </strong>|
    <span class="col-6">@Model.ParselNumber</span>
</li>
<li class="row">
    <strong class="col-5">Sheet Number </strong>|
    <span class="col-6">@Model.SheetNumber</span>
</li>
<li class="row">
    <strong class="col-5">Price per Square Metersize </strong>|
    <span class="col-6">@Model.PricePerSquareMetersize <i class="fa fa-dollar"></i></span>
</li>
<li class="row">
    <strong class="col-5">Title Deed State </strong>|
    <span class="col-6">@(Model.TitleDeedState ? "Yes" : "No")</span>
</li>
<li class="row">
    <strong class="col-5">Zoning Status </strong>|
    <span class="col-6">@(Model.ZoningStatus ? "Yes" : "No")</span>
</li>